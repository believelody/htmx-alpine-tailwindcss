<a hx-get="{{ href }}" hx-push-url="true" hx-swap="innerHTML" hx-target="#content" class="inline-flex items-center cursor-pointer hover:border-b-2 hover:border-b-{{withDefault color 'neutral'}}-500 text-{{withDefault color 'neutral'}}-500">
    <span class="mr-2">{{label}}</span>
    {{#if icon}}
        <span>{{ icon }}</span>
    {{/if}}
</a>